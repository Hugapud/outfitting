<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.controller.KBExperience">
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
            <HBox alignment="CENTER_LEFT" spacing="5.0">
               <children>
                        <Label minWidth="-Infinity" prefHeight="15.0" prefWidth="53.0" text="船舶类型" />
                        <ComboBox fx:id="experienceshipType1" editable="true" prefWidth="150.0" promptText="船舶类型" />
                        <Label minWidth="-Infinity" prefHeight="15.0" prefWidth="54.0" text="舾装区域">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin></Label>
                        <TextField fx:id="experienceOutfittingTypeTextField1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="10.0" prefWidth="150.0" promptText="舾装区域" />
                        <Label minWidth="-Infinity" prefHeight="19.0" prefWidth="89.0" text="经验知识名称">
                            <font>
                                <Font size="14.0" />
                            </font>
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                        </Label>
                        <TextField fx:id="experienceNameTextField1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="10.0" prefWidth="150.0" promptText="经验知识名称" />
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
                <Separator />
                <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" spacing="20.0" VBox.vgrow="ALWAYS">
                    <children>
                        <Button mnemonicParsing="false" onAction="#showAddExperience" text="添加" />
                        <Button mnemonicParsing="false" onAction="#handleEditExperience" text="修改" />
                        <Button mnemonicParsing="false" onAction="#handleDeleteExperience" text="删除" />
                        <Button cancelButton="true" mnemonicParsing="false" onAction="#handleResetExperience" text="重置" />
                        <Button defaultButton="true" disable="true" mnemonicParsing="false" onAction="#handleExperienceQuery" text="搜索" visible="false" />
                        <TextField fx:id="experienceQueryTextField" disable="true" prefWidth="300.0" visible="false">
                            <HBox.margin>
                                <Insets left="30.0" />
                            </HBox.margin>
                        </TextField>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <Separator />
                <HBox VBox.vgrow="ALWAYS">
                    <children>
                        <TableView fx:id="experienceTable1" fixedCellSize="25.0" tableMenuButtonVisible="true" HBox.hgrow="ALWAYS">
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                            <columns>
                                <TableColumn fx:id="e52" prefWidth="75.0" text="船舶类型" />
                                <TableColumn fx:id="e53" prefWidth="75.0" text="舾装区域" />
                                <TableColumn fx:id="e54" prefWidth="75.0" text="经验知识名称" />
                                <TableColumn fx:id="e55" prefWidth="75.0" text="经验知识内容" />
                                <TableColumn fx:id="expFilePathTC" prefWidth="75.0" text="经验文件路径" />
                            </columns>
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </TableView>
                        <VBox spacing="5.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Label minHeight="-Infinity" minWidth="-Infinity" prefHeight="15.0" prefWidth="80.0" text="经验知识内容" />
                                <TextArea fx:id="experienceTextArea1" promptText="经验知识内容" wrapText="true" VBox.vgrow="ALWAYS" />
                            </children>
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </padding>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
