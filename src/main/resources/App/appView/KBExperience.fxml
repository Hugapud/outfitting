<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.controller.KBExperience">
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <GridPane>
                    <children>
                        <TextField fx:id="experienceOutfittingTypeTextField1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="10.0" prefWidth="150.0" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
                        <TextField fx:id="experienceNameTextField1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="10.0" prefWidth="150.0" GridPane.columnIndex="5" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
                        <Label text="船舶类型" />
                        <Label text="舾装区域" GridPane.columnIndex="2" />
                        <Label text="经验知识名称" GridPane.columnIndex="4">
                            <font>
                                <Font size="14.0" />
                            </font>
                        </Label>
                        <ComboBox fx:id="experienceshipType1" editable="true" prefWidth="150.0" GridPane.columnIndex="1" />
                    </children>
                    <columnConstraints>
                        <ColumnConstraints halignment="RIGHT" />
                        <ColumnConstraints hgrow="ALWAYS" />
                        <ColumnConstraints halignment="RIGHT" />
                        <ColumnConstraints hgrow="ALWAYS" />
                        <ColumnConstraints halignment="RIGHT" />
                        <ColumnConstraints hgrow="ALWAYS" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" />
                    </rowConstraints>
                    <VBox.margin>
                        <Insets left="10.0" />
                    </VBox.margin>
                </GridPane>
                <Separator />
                <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" prefHeight="40.0" spacing="20.0" VBox.vgrow="ALWAYS">
                    <children>
                        <TextField fx:id="experienceQueryTextField" prefWidth="300.0">
                            <HBox.margin>
                                <Insets left="30.0" />
                            </HBox.margin>
                        </TextField>
                        <Button defaultButton="true" mnemonicParsing="false" onAction="#handleExperienceQuery" text="搜索" />
                        <Button mnemonicParsing="false" onAction="#showAddExperience" text="添加" />
                        <Button mnemonicParsing="false" onAction="#handleEditExperience" text="修改" />
                        <Button mnemonicParsing="false" onAction="#handleDeleteExperience" text="删除" />
                        <Button cancelButton="true" mnemonicParsing="false" onAction="#handleResetExperience" text="重置" />
                    </children>
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </HBox>
                <Separator />
                <HBox VBox.vgrow="ALWAYS">
                    <children>
                        <TableView fx:id="experienceTable1" fixedCellSize="25.0" tableMenuButtonVisible="true" HBox.hgrow="ALWAYS">
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                            <columns>
                                <TableColumn fx:id="e52" prefWidth="75.0" text="船舶类型" />
                                <TableColumn fx:id="e53" prefWidth="75.0" text="舾装区域" />
                                <TableColumn fx:id="e54" prefWidth="75.0" text="经验知识名称" />
                                <TableColumn fx:id="e55" prefWidth="75.0" text="经验知识内容" />
                                <TableColumn fx:id="expFilePathTC" prefWidth="75.0" text="经验文件路径" />
                            </columns>
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </TableView>
                        <VBox spacing="5.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Label minHeight="-Infinity" minWidth="-Infinity" prefHeight="15.0" prefWidth="80.0" text="经验知识内容" />
                                <TextArea fx:id="experienceTextArea1" wrapText="true" VBox.vgrow="ALWAYS" />
                            </children>
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </padding>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
