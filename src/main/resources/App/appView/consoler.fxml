                                <HBox alignment="CENTER_LEFT" pickOnBounds="false" spacing="10.0">
                                    <children>
                                        <GridPane alignment="CENTER_LEFT" maxWidth="-Infinity" minWidth="-Infinity"
                                                  pickOnBounds="false" prefWidth="280.0" vgap="8.0">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="115.0" minWidth="10.0"
                                                                   prefWidth="108.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="154.0" minWidth="10.0"
                                                                   prefWidth="147.0"/>
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
