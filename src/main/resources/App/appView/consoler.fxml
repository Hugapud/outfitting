<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="App.formulalib.Consoler">
    <children>
        <TitledPane collapsible="false" layoutX="30.0" layoutY="21.0" text="公式编辑" AnchorPane.topAnchor="21.0">
