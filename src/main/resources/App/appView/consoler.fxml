                <StackPane prefWidth="788.0">
                    <children>
                        <VBox prefHeight="250.0" spacing="30.0">
                            <children>
                                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                                    <children>
                                        <VBox spacing="10.0">
                                            <children>
                                                <Label text="待求变量"/>
                                                <TextField fx:id="tfStringNewVariable" prefHeight="30.0"
                                                           prefWidth="100.0" promptText="请输入待求变量名"/>
                                            </children>
                                        </VBox>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="=">
                                            <font>
                                                <Font size="46.0"/>
                                            </font>
                                            <HBox.margin>
                                                <Insets top="5.0"/>
                                            </HBox.margin>
                                        </Text>
                                        <VBox spacing="10.0">
                                            <children>
                                                <Label text="计算代数式"/>
                                                <TextField fx:id="tfStringNewAlgebraic" prefHeight="30.0"
                                                           prefWidth="300.0" promptText="请输入计算代数式"/>
                                            </children>
                                        </VBox>
                                        <VBox layoutX="10.0" layoutY="10.0" spacing="10.0">
                                            <children>
                                                <Label text="公式备注"/>
                                                <TextField fx:id="tfStringNewFormulaRemark" prefHeight="30.0"
                                                           prefWidth="161.0" promptText="请输入备注"/>
                                            </children>
                                        </VBox>
                                    </children>
                                </HBox>
                                <VBox spacing="20.0"
