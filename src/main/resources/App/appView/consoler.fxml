                                        <FlowPane prefHeight="200.0" prefWidth="200.0">
                                            <children>
                                                <Label text="是否使用规则："/>
                                                <CheckBox fx:id="cbNewIsRuled" mnemonicParsing="false"/>
                                            </children>
                                        </FlowPane>
                                        <HBox spacing="40.0">
                                            <children>
                                                <VBox spacing="10.0">
                                                    <children>
                                                        <Label text="规则变量"/>
                                                        <TextField fx:id="tfStringNewRestrictedVariable"
                                                                   prefWidth="100.0"/>
                                                    </children>
                                                </VBox>
                                                <HBox alignment="BOTTOM_LEFT" spacing="10.0">
                                                    <children>
                                                        <Label text="下限值"/>
                                                        <TextField fx:id="tfDoubleNewLowerBound"/>
                                                    </children>
                                                </HBox>
                                                <HBox alignment="BOTTOM_LEFT" spacing="10.0">
                                                    <children>
                                                        <Label text="上限值"/>
                                                        <TextField fx:id="tfDoubleNewUpperBound"/>
                                                    </children>
                                                </HBox>
                                            </children>
                                        </HBox>
                                        <VBox spacing="10.0">
                                            <children>
                                                <Label text="公式备注"/>
                                                <TextField fx:id="tfStringNewRuleDescription"/>
                                            </children>
                                        </VBox>
                                    </children>
                                </VBox>
                                <StackPane prefHeight="150.0" prefWidth="200.0">
                                    <children>
                                        <Button fx:id="submitFormula" mnemonicParsing="false" text="添加公式"/>
