                                                                <TableColumn fx:id="tcTableFormulaCol5" editable="false"
                                                                             minWidth="1.0" prefWidth="88.0"
                                                                             resizable="false" text="判断规则"/>
                                                                <TableColumn fx:id="tcTableFormulaCol6" editable="false"
                                                                             prefWidth="205.0" text="规则表达式"/>
                                                                <TableColumn fx:id="tcFormulaLastColumn"
                                                                             editable="false" prefWidth="131.0"
                                                                             resizable="false" text="公式说明"/>
                                                            </columns>
                                                        </TableView>
                                                    </children>
                                                </StackPane>
                                            </children>
                                            <HBox.margin>
                                                <Insets bottom="10.0" top="10.0"/>
                                            </HBox.margin>
                                        </VBox>
                                        <VBox alignment="CENTER" prefWidth="100.0" spacing="60.0">
                                            <children>
                                                <Button fx:id="btEditFormula" contentDisplay="CENTER"
                                                        mnemonicParsing="false" prefHeight="35.0" prefWidth="80.0"
                                                        text="编辑公式"/>
                                                <Button fx:id="btDeleteFormula" contentDisplay="CENTER"
                                                        mnemonicParsing="false" prefHeight="35.0" prefWidth="80.0"
                                                        text="删除公式"/>
                                            </children>
                                        </VBox>
                                    </children>
                                    <StackPane.margin>
                                        <Insets left="20.0"/>
                                    </StackPane.margin>
                                </HBox>
                            </children>
                        </StackPane>
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
