                                        <Button fx:id="btDeleteVariable" mnemonicParsing="false" prefHeight="35.0"
                                                prefWidth="80.0" text="删除变量"/>
                                    </children>
                                    <padding>
                                        <Insets bottom="20.0" top="20.0"/>
                                    </padding>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </StackPane>
