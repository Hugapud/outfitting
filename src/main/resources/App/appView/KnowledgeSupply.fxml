<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="knowledgeSupplyBorderPane" maxHeight="1.7976931348623157E308"
            maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="1000.0" stylesheets="@KnowledgeSupply.css"
            xmlns="http://javafx.com/javafx/8" fx:controller="App.controller.KnowledgeSupply">
    <left>
        <VBox fx:id="leftVbox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
              BorderPane.alignment="CENTER">
            <children>
                <VBox VBox.vgrow="ALWAYS">
                    <children>
                        <Button fx:id="recommendBtn" mnemonicParsing="false" onAction="#recommendAction"
                                prefHeight="40.0" prefWidth="40.0"/>
                        <Button fx:id="supplyBtn" mnemonicParsing="false" onAction="#supplyAction" prefHeight="40.0"
                                prefWidth="40.0"/>
                    </children>
                    <padding>
                        <Insets top="40.0"/>
                    </padding>
                </VBox>
                <Button fx:id="backBtn" cancelButton="true" mnemonicParsing="false" onAction="#backAction"
                        prefHeight="40.0" prefWidth="40.0"/>
            </children>
        </VBox>
    </left>
</BorderPane>
