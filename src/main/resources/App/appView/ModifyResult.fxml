<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.controller.ModifyResult">
   <children>
      <VBox alignment="TOP_CENTER" prefWidth="700.0">
         <children>
            <VBox alignment="TOP_CENTER" VBox.vgrow="ALWAYS">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="5.0">
                     <children>
                        <Label text="选择项目" />
                        <ComboBox editable="true" prefWidth="150.0" />
                        <Button mnemonicParsing="false" text="..." />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" spacing="30.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Button mnemonicParsing="false" text="检查">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Button>
                        <Label text="检查出未在可行域内的参数并高亮显示" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </HBox>
                  <TableView>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <columns>
                        <TableColumn prefWidth="75.0" text="舾装件" />
                        <TableColumn editable="false" prefWidth="75.0" text="参数名称" />
                        <TableColumn editable="false" prefWidth="75.0" text="参数描述" />
                        <TableColumn prefWidth="75.0" text="参数值" />
                        <TableColumn prefWidth="75.0" text="参数可行域" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                  <Button mnemonicParsing="false" text="修改" />
                  <Button mnemonicParsing="false" text="下一步" />
               </children>
               <padding>
                  <Insets top="30.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
