<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.controller.ModifyResult">
   <children>
      <VBox maxHeight="1.7976931348623157E308">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="5.0">
               <children>
                  <Label alignment="CENTER_RIGHT" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="60.0" text="选择项目">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="projChooserCB" prefWidth="350.0" />
                  <Label alignment="CENTER_RIGHT" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="150.0" text="选择版本">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="versionChooserCB" prefWidth="200.0" />
               </children>
               <padding>
                  <Insets left="40.0" />
               </padding>
               <VBox.margin>
                  <Insets right="20.0" />
               </VBox.margin>
            </HBox>
            <Separator>
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Separator>
            <GridPane alignment="CENTER_LEFT" vgap="5.0">
               <children>
                  <Label text="项目：">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Label>
                  <Label text="版本：" GridPane.rowIndex="1">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Label>
                  <Label fx:id="projLabel" GridPane.columnIndex="1" />
                  <Label fx:id="versionLabel" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
               <columnConstraints>
                  <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="458.0" minWidth="10.0" prefWidth="437.0" />
               </columnConstraints>
               <padding>
                  <Insets left="40.0" />
               </padding>
               <rowConstraints>
                  <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
               </rowConstraints>
            </GridPane>
            <Separator>
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Separator>
            <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="20.0">
               <children>
                  <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" spacing="20.0">
                     <children>
                        <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" spacing="5.0">
                           <children>
                              <TableView fx:id="projParamValueTV" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tableMenuButtonVisible="true">
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                                 <columns>
                                    <TableColumn fx:id="outfittingNameTC" prefWidth="75.0" text="舾装件名称" />
                                    <TableColumn fx:id="paramNameTC" editable="false" prefWidth="75.0" text="参数名称" />
                                    <TableColumn fx:id="paramDescriptionTC" editable="false" prefWidth="75.0" text="参数描述" />
                                    <TableColumn fx:id="paramValueTC" prefWidth="75.0" text="参数值" />
                                    <TableColumn prefWidth="75.0" text="参数可行域" />
                                 </columns>
                              </TableView>
                           </children>
                        </VBox>
                     </children>
                     <padding>
                        <Insets left="40.0" />
                     </padding>
                  </VBox>
                  <VBox alignment="TOP_CENTER" spacing="5.0">
                     <HBox.margin>
                        <Insets bottom="10.0" right="20.0" />
                     </HBox.margin>
                     <children>
                        <TableView fx:id="projParamValueTV1" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tableMenuButtonVisible="true">
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                           <columns>
                              <TableColumn fx:id="outfittingNameTC1" prefWidth="75.0" text="舾装件名称" />
                              <TableColumn fx:id="paramNameTC1" editable="false" prefWidth="75.0" text="参数名称" />
                              <TableColumn fx:id="paramDescriptionTC1" editable="false" prefWidth="75.0" text="参数描述" />
                              <TableColumn fx:id="paramValueTC1" prefWidth="75.0" text="参数值" />
                              <TableColumn prefWidth="75.0" text="参数可行域" />
                           </columns>
                        </TableView>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
