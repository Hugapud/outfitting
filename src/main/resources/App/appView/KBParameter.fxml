<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.controller.KBParameter">
   <children>
      <VBox spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TableView fx:id="paramTV" VBox.vgrow="ALWAYS">
              <columns>
                  <TableColumn fx:id="outfittingNameTC" prefWidth="75.0" text="舾装件名称" />
                <TableColumn fx:id="paramNameTC" prefWidth="75.0" text="参数名称" />
                  <TableColumn fx:id="paramTypeTC" prefWidth="75.0" text="参数类型" />
                  <TableColumn fx:id="paramDescTC" prefWidth="75.0" text="参数描述" />
                  <TableColumn fx:id="paramScopeTC" prefWidth="75.0" text="参数范围" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </TableView>
            <HBox alignment="CENTER" spacing="30.0">
               <children>
                  <Button fx:id="addBtn" mnemonicParsing="false" onAction="#addAction" text="添加" />
                  <Button fx:id="modifyBtn" mnemonicParsing="false" onAction="#modifyAction" text="修改" />
               </children>
               <padding>
                  <Insets bottom="5.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
