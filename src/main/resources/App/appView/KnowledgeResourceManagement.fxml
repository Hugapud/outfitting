<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="knowledgeResourceBorderPane" maxHeight="1.7976931348623157E308"
            maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="1100.0"
            stylesheets="@KnowledgeResourceManagement.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="App.controller.KnowledgeResourceManagement">
    <left>
        <VBox BorderPane.alignment="CENTER">
            <children>
                <VBox fx:id="knowledgeResourceManaVbox" VBox.vgrow="ALWAYS">
                    <children>
                        <Button fx:id="KBBtn" mnemonicParsing="false" onAction="#goKBMana" prefHeight="40.0"
                                prefWidth="40.0"/>
                        <Button fx:id="DBBtn" mnemonicParsing="false" onAction="#goDBMana" prefHeight="40.0"
                                prefWidth="40.0"/>
                    </children>
                    <padding>
                        <Insets top="40.0"/>
                    </padding>
                </VBox>
                <Button fx:id="backHomeBtn" cancelButton="true" mnemonicParsing="false" onAction="#backHome"
                        prefHeight="40.0" prefWidth="40.0"/>
            </children>
        </VBox>
    </left>
</BorderPane>
