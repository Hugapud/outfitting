<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="createProjAnchorPane" stylesheets="@CreateProject.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.controller.CreateProject">
    <children>
        <VBox alignment="TOP_CENTER" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox alignment="CENTER_LEFT" spacing="10.0">
                    <children>
                        <Label text="选择项目">
                            <HBox.margin>
                                <Insets left="40.0" />
                            </HBox.margin>
                        </Label>
                        <ComboBox fx:id="projectComboBox" prefWidth="150.0" />
                        <Label fx:id="projectLabel" />
                        <Button fx:id="deleteBtn" mnemonicParsing="false" onAction="#deleteProjectAction" />
                    </children>
                </HBox>
                <Separator />
                <GridPane>
                    <children>
                        <Label text="项目名称">
                            <font>
                                <Font size="19.0" />
                            </font>
                        </Label>
                        <TextField fx:id="projNameTF" prefWidth="250.0" GridPane.columnIndex="1">
                            <GridPane.margin>
                                <Insets left="10.0" />
                            </GridPane.margin>
                        </TextField>
                        <Label text="创建时间" GridPane.rowIndex="2">
                            <font>
                                <Font size="19.0" />
                            </font>
                        </Label>
                        <TextField fx:id="projCreateTimeTF" editable="false" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                            <GridPane.margin>
                                <Insets left="10.0" />
                            </GridPane.margin>
                        </TextField>
                        <Label text="修改时间" GridPane.rowIndex="3">
                            <font>
                                <Font size="19.0" />
                            </font>
                        </Label>
                        <TextField fx:id="projModifyTimeTF" editable="false" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                            <GridPane.margin>
                                <Insets left="10.0" />
                            </GridPane.margin>
                        </TextField>
                        <Label text="负责人" GridPane.rowIndex="4">
                            <font>
                                <Font size="19.0" />
                            </font>
                        </Label>
                        <TextField fx:id="projPersonInChargeTF" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                            <GridPane.margin>
                                <Insets left="10.0" />
                            </GridPane.margin>
                        </TextField>
                        <Label text="备注" GridPane.rowIndex="5">
                            <font>
                                <Font size="19.0" />
                            </font>
                        </Label>
                        <TextArea fx:id="projRemarkTF" prefHeight="150.0" prefWidth="250.0" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="5">
                            <GridPane.margin>
                                <Insets left="10.0" />
                            </GridPane.margin>
                        </TextArea>
                        <HBox alignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="6">
                            <GridPane.margin>
                                <Insets />
                            </GridPane.margin>
                        </HBox>
                        <Label text="请按照部门内部命名规范填写" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="TOP">
                            <GridPane.margin>
                                <Insets left="10.0" />
                            </GridPane.margin>
                        </Label>
                        <HBox alignment="CENTER" spacing="50.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
                            <children>
                                <Button fx:id="createProjResetBtn" mnemonicParsing="false" onAction="#displayDBData">
                                    <font>
                                        <Font size="13.0" />
                                    </font>
                                </Button>
                                <Button fx:id="createProjSaveBtn" mnemonicParsing="false" onAction="#saveAction">
                                    <font>
                                        <Font size="13.0" />
                                    </font>
                                </Button>
                                <Button fx:id="createProjNextBtn" mnemonicParsing="false" onAction="#nextStepAction">
                                    <font>
                                        <Font size="13.0" />
                                    </font>
                                </Button>
                            </children>
                        </HBox>
                        <Button fx:id="mtrBtn" mnemonicParsing="false" onAction="#modifyTimeRefresh" wrapText="true" GridPane.columnIndex="2" GridPane.rowIndex="3">
                            <GridPane.margin>
                                <Insets left="10.0" />
                            </GridPane.margin>
                        </Button>
                        <Button fx:id="ctrBtn" mnemonicParsing="false" onAction="#createTimeRefresh" wrapText="true" GridPane.columnIndex="2" GridPane.rowIndex="2">
                            <GridPane.margin>
                                <Insets left="10.0" />
                            </GridPane.margin>
                        </Button>
                    </children>
                    <columnConstraints>
                        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="80.0" prefWidth="150.0" />
                        <ColumnConstraints fillWidth="false" halignment="LEFT" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="273.0" />
                        <ColumnConstraints fillWidth="false" halignment="LEFT" hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="174.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" valignment="BOTTOM" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" valignment="BOTTOM" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" valignment="CENTER" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" valignment="CENTER" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" valignment="CENTER" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="150.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                </GridPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
