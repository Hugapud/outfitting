<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="outfittingDesignBorderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.controller.OutfittingDesign">
   <left>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <VBox VBox.vgrow="ALWAYS">
               <children>
                          <Button fx:id="outfittingItemSelectionButton" mnemonicParsing="false" onAction="#goOutfittingItemSelection" prefHeight="40.0" prefWidth="40.0">
                     <VBox.margin>
                        <Insets top="40.0" />
                     </VBox.margin>
                  </Button>
                          <Button fx:id="layoutDesignButton" mnemonicParsing="false" onAction="#goLayoutDesign" prefHeight="40.0" prefWidth="40.0" />
                          <Button fx:id="designReviewButton" mnemonicParsing="false" onAction="#goDesignReview" prefHeight="40.0" prefWidth="40.0" />
                  <Button mnemonicParsing="false" onAction="#goExport" prefHeight="40.0" prefWidth="40.0" />
               </children>
            </VBox>
                    <Button fx:id="backHomeBtn" cancelButton="true" mnemonicParsing="false" onAction="#backHome" prefHeight="40.0" prefWidth="40.0" />
         </children>
      </VBox>
   </left>
</BorderPane>
